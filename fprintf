#include <stdio.h>
int main(void)
{
FILE *fp;
int number = 20;
double grade = 4.3;
char name[ ] = “홍길동”;
fp = fopen(“sample.txt”, “w”);
if(fp == NULL) {
printf(“파일 열기 실패\n”);
return 0;
}
fprintf( fp, “%s \n”, name);
fprintf( fp, “%d \n”, number);
fprintf( fp, “%f \n”, grade);
fclose(fp);
return 0;
}
